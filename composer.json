{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "achse/geth-jsonrpc-php-client": "^3.0",
        "denpa/php-bitcoinrpc": "^2.0",
        "dingo/api": "2.0.x@dev",
        "earnp/getui": "dev-master",
        "fideloper/proxy": "^4.0",
        "jiguang/jmessage": "^1.1",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravist/geecaptcha": "^1.1",
        "nicolaslopezj/searchable": "1.*",
        "predis/predis": "^1.1",
        "sc0vu/web3.php": "dev-master",
        "sixteener/taobaosdk": "~1.0",
        "swooletw/laravel-swoole": "^2.5",
        "tucker-eric/eloquentfilter": "^1.3",
        "tymon/jwt-auth": "1.0.*",
        "web3p/ethereum-tx": "^0.3.3",
        "zgldh/qiniu-laravel-storage": "^0.10.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files":[
            "app/Handlers/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
